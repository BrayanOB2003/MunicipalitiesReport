<Window x:Class="MunicipalitiesReport.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MunicipalitiesReport"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Cargar información" HorizontalAlignment="Left" Margin="63,19,0,0" VerticalAlignment="Top" Height="35" Width="119" Click="SearchFile"/>
        <DataGrid x:Name="table" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,97,0,0"/>
        <Button Content="Ver gráfico" HorizontalAlignment="Left" Margin="215,19,0,0" VerticalAlignment="Top" Height="35" Width="113" Click="ShowGraphic"/>

    </Grid>
</Window>
